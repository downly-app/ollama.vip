{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devPath": "http://localhost:1421",
    "distDir": "../dist"
  },
  "package": {
    "productName": "ollama-pro",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "show": true,
        "maximize": true,
        "minimize": true,
        "unmaximize": true,
        "unminimize": true,
        "startDragging": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "vip.ollama.app",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "category": "Productivity",
      "shortDescription": "Professional AI model management interface",
      "longDescription": "Ollama Pro is a professional desktop application for managing and interacting with AI models through an intuitive interface.",
      "copyright": "Copyright Â© 2024 Ollama Pro. All rights reserved.",
      "publisher": "Ollama Pro Team",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "10.13",
        "exceptionDomain": ""
      },
      "deb": {
        "depends": []
      },
      "rpm": {
        "epoch": 0,
        "files": {},
        "release": "1"
      }
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 1000,
        "minHeight": 900,
        "minWidth": 1400,
        "resizable": true,
        "title": "ollama-pro",
        "width": 1400,
        "decorations": false,
        "transparent": true,
        "center": true
      }
    ],
    "macOSPrivateApi": true
  }
}
